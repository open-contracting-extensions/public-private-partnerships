msgid "conflictOfInterest"
msgstr ""

#: ../../docs/reference/codelists.md:133
msgid "Conflict of interest"
msgstr ""

#: ../../docs/reference/codelists.md:133
msgid "Documentation of conflicts of interest declared or uncovered."
msgstr ""

